  <body ng-app = "personfollowerslist" >
 <nav class="navbar navbar-default mynav">
  <div class="container-fluid">
    <ul class="nav navbar-nav ">
      <li ><a href="/userhomepage"><span class="glyphicon glyphicon-home"></span> Home</a></li>
      <li><a href="#"><span class="glyphicon glyphicon-flash"></span> Moments</a></li>
      <li><a href="#"><span class="glyphicon glyphicon-bell"></span> Notifications</a></li>
      <li><a href="#"> <span class="glyphicon glyphicon-envelope"></span> Messages</a></li>
    </ul>
    <div style = "margin-left:600px;">
    <ul class="nav navbar-nav navbar-center" style="margin-top:10px;">
    <li><img src = "/images/favicon.ico" height = "20px" width = "20px"></li>
    </ul>
    </div>
   
    <ul class="nav navbar-nav navbar-right">
     
      <li><form class="navbar-form" role="search" action="/hashsearch">
        <div class="input-group" >
            <input type="text" class="form-control" placeholder="Search" name  = "search" >
            <div class="input-group-btn">
                <button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
            </div>
        </div>
        </form></li>
     <li><a href = "/logout"><span class="glyphicon glyphicon-log-out"></span> Logout</a></li>
    </ul>
  </div>
</nav>
  
  
<div class="container-fluid" style="margin-top:-20px">
 <div style="height:170px;background:#2B7BB8">
 </div>

 <div class="row" style="background:#ffffff"  ng-init="initheader()" ng-controller = "inheader"  >
 <div class="col-md-3">
 <img ng-if="profilepic" style="margin-left:25px;margin-top:-90px;border:solid 5px;border-color:#ffffff;border-radius:10px;" data-ng-src = "{{profilepic}}" id = "profilepic" height = "200px" width = "200px"/ >
 <div ng-if="!profilepic" style="background:#2B7BB8;margin-left:25px;margin-top:-90px;border:solid 5px;border-color:#ffffff;border-radius:10px;height:200px;width:200px;background-image:url('/images/cam.png');"></div>
 
 </div>
 <div class="col-md-3" >
 <div class = "col-md-4">
    <h5><small ng-click = "tweetlist();" class="mycountheading">TWEETS</small></h5>
    <h4 class="mycount"  ng-model = "tweetupdate">{{tweetupdate}}</h4>     
    </div>
    <div class = "col-md-4 ">
    <h5><small ng-click = "followinglist();" class="mycountheading">FOLLOWING</small></h5>
      <h4 class="mycount"  ng-model = "followingupdate" > {{followingupdate}}</h4>
    </div>
    <div class = "col-md-4">
    <h5><small ng-click = "followerslist();" class="mycountheading">FOLLOWERS</small></h5>
    <h4 class="mycount" ng-model = "followersupdate" >{{followersupdate}}</h4>
    </div>
 </div>
 <div class="col-md-6" >
 </div>
 </div>
 </div>
<input type = "hidden" value = "<%= userid %>" id  = "hiddendata">


 <div class="container-fluid" style="margin-top:-30px;background:#F5F8FA;">
  <div class="row" >
  <div class="col-md-3" style="margin-left:-270px;margin-top:30px" ng-controller="profile" ng-init="initprofile()">

    <h4 class="text-primary" ng-if = "users[0].fullname"><span class="glyphicon glyphicon-user">{{users[0].fullname}}<br/>&nbsp;<small>{{profile[0].username}}</small></span> </h4> 
    <h5 class="text-primary"  ng-if = "profile[0].bio"><span class="glyphicon glyphicon-education">{{profile[0].bio}}</span> </h5>
    <h5 class="text-primary"   ng-if = "profile[0].location"><span class="glyphicon glyphicon-map-marker">{{profile[0].location}}</span>  </h5>
    <h5 class="text-primary"   ng-if = "profile[0].website"><span class="glyphicon glyphicon-link">{{profile[0].website}}</span> </h5> 
     <h5 class="text-primary" ng-if = "profile[0].birthday"><span class="glyphicon glyphicon-dashboard">{{profile[0].birthday| date :'MM/dd/yyyy'}}</span></h5>
    <h5 class="text-primary"  ng-if = "users[0].email"><span class="glyphicon glyphicon-envelope">{{users[0].email}}</span></h5> 
    <h5 class="text-primary" ng-if = "users[0].phonenum" ><span class="glyphicon glyphicon-phone">{{users[0].countrycode}}  {{users[0].phonenum}}</span> </h5> 

  </div>
  <div style="margin-left:270px;margin-top:-120px;" class="col-md-9" ng-init="initlist()" ng-controller = "list">
  <div  ng-hide= "showfollowers">
  <div class="col-md-4" style= "overflow: hidden;width:300px;height:200px;margin-left:10px;margin-bottom:10px;border-radius: 5px;" ng-repeat="x in followerdetails">
  <div style="height:50%;width:350px;background:#2B7BB8">
  </div>
  <div style="height:20%;width:300px;background:#ffffff">
  <div class=row>
  <div class="col-md-5">
  <img ng-if="x.profilepic" data-ng-src = {{x.profilepic}} height = "60px" width = "60px" style="margin-top:-25px;margin-left:10px;border:solid 3px;border-color:#ffffff;border-radius:10px;"/>
  <div ng-if="!x.profilepic" style="height:60px;width:60px;margin-top:-25px;background:#2B7BB8;margin-left:10px;border:solid 3px;border-color:#ffffff;border-radius:10px;"><img src = "/images/cam.png" style="margin-left:-22px;margin-top:-22px" height="100px" width="100px"/></div>
  </div>
  <div class="col-md-6" style="margin-top:5px">
  <i style="margin-left:5px" ng-if = "arrayflags[$index]==true" class="glyphicon glyphicon-cog gray"></i><button class="btn btn-primary pull-right" ng-if = "arrayflags[$index]==true">Following</follow>
  <button class ="btn btn-default pull-right myfollowbtn "  ng-click = "follow($index);" ng-if = "arrayflags[$index]==false" ><span class="glyphicon glyphicon-user blue"></span>Follow</follow>
  </div>
  </div>
  </div>
  <div style="margin-top:-20px;height:30%;width:300px;background:#ffffff">
  <h3 style="margin-left:10px;" ng-click = "clickperson($index)"><strong class="mystrong">{{x.fullname}}</strong><br/><small>
         {{x.username}}</small></h3> 
  </div>
  </div>
  <p ng-hide = "nosuggestions">No followers are available</p>
  </div>
               
    <div ng-hide= "showtweets">
 <div class="panel-body" style ="border-radius:10px;background:#ffffff"	>
   <div class= "row" ng-repeat="x in feed">
   <div class="col-md-1">
    <img src = "/images/tweetegg.png" style ="border-radius:5px;height:43px;width:43px"/>
    
   </div>
   <div class="col-md-11">
   <dir id="dir" content="x.value"></dir>
   </div>  
   <div style="border:solid 1px;border-color:#F5F8FA"> </div>
   </div> 
 
     <p ng-hide = "notweets">No tweets are available</p>
    
    </div> 
     </div> 
  </div>
    </div>
      
    </div>
    
  
  

  </div>
  </div>
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
